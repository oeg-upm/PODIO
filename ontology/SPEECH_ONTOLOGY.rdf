###  http://w3id.org/speech#hasIdeology
speech:hasIdeology rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( speech:Speech
                                               speech:TwitterUser
                                               foaf:Agent
                                             )
                               ] ;
                   rdfs:range [ owl:intersectionOf ( skos:Concept
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty skos:inScheme ;
                                                       owl:hasValue <https://skos.um.es/unescothes/>
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
                   rdfs:comment "The ideology an Agent or a Speech has." ;
                   rdfs:label "has ideology" .


###  http://w3id.org/speech#hasOfficialHashtag
speech:hasOfficialHashtag rdf:type owl:ObjectProperty ;
                          owl:inverseOf speech:officialhashtagOf ;
                          rdfs:domain speech:PoliticalPartyManifesto ;
                          rdfs:range speech:Hashtag ;
                          rdfs:comment "Official hashtag of the electoral campaign." ;
                          rdfs:label "has official hashtag" .


###  http://w3id.org/speech#hasPoliticalCategory
speech:hasPoliticalCategory rdf:type owl:ObjectProperty ;
                            owl:inverseOf speech:politicalCategoryOf ;
                            rdfs:domain speech:PoliticalProposal ;
                            rdfs:range skos:Concept ;
                            rdfs:comment "The category to which a political proposal belongs." ;
                            rdfs:label "has political category" .


###  http://w3id.org/speech#ideologyOf
speech:ideologyOf rdf:type owl:ObjectProperty ;
                  rdfs:domain skos:Concept ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( speech:Speech
                                             speech:TwitterUser
                                             foaf:Agent
                                           )
                             ] ;
                  rdfs:comment "The elements that have this ideology. They can be both Agent-s and Speech-es." ;
                  rdfs:label "ideology of" .


###  http://w3id.org/speech#officialhashtagOf
speech:officialhashtagOf rdf:type owl:ObjectProperty ;
                         rdfs:domain speech:Hashtag ;
                         rdfs:range speech:PoliticalPartyManifesto ;
                         rdfs:comment "The political manifesto that officially proposes the hashtag for the electoral campaign." ;
                         rdfs:label "official hashtag of" .


###  http://w3id.org/speech#politicalCategoryOf
speech:politicalCategoryOf rdf:type owl:ObjectProperty ;
                           rdfs:domain skos:Concept ;
                           rdfs:range speech:PoliticalProposal ;
                           rdfs:comment "Political proposals that address this issue." ;
                           rdfs:label "political category of" .


###  http://w3id.org/speech#proposedAsPoliticalCandidateBy
speech:proposedAsPoliticalCandidateBy rdf:type owl:ObjectProperty ;
                                      owl:inverseOf speech:proposesPoliticalCandidate ;
                                      rdfs:domain foaf:Agent ;
                                      rdfs:range speech:PoliticalPartyManifesto ;
                                      rdfs:comment "The political manifesto that presents this agent as candidate to the elections." ;
                                      rdfs:label "proposed as political candidate by" .


###  http://w3id.org/speech#proposesPoliticalCandidate
