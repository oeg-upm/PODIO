                                  rdfs:domain speech:PoliticalPartyManifesto ;
                                  rdfs:range foaf:Agent ;
                                  rdfs:comment "The political candidate presented by the political party in its manifesto for the electoral campaign." ;
                                  rdfs:label "proposes political candidate" .


###  http://w3id.org/speech#retweetedBy
speech:retweetedBy rdf:type owl:ObjectProperty ;
                   owl:inverseOf speech:retweets ;
                   rdfs:domain speech:Tweet ;
                   rdfs:range speech:TwitterUser ;
                   rdfs:comment "The twitter account that retweeted this tweet." ;
                   rdfs:label "is retweeted by" .


###  http://w3id.org/speech#retweets
speech:retweets rdf:type owl:ObjectProperty ;
