###  http://w3id.org/speech#hasIdeology
:hasIdeology rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Speech
                                         :TwitterUser
                                         <http://xmlns.com/foaf/0.1/Agent>
                                       )
                         ] ;
             rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                 owl:hasValue <https://skos.um.es/unescothes/>
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
             rdfs:comment "The ideology an Agent or a Speech has." ;
             rdfs:label "has ideology" .


###  http://w3id.org/speech#hasOfficialHashtag
:hasOfficialHashtag rdf:type owl:ObjectProperty ;
                    owl:inverseOf :officialhashtagOf ;
                    rdfs:domain :PoliticalPartyManifesto ;
                    rdfs:range :Hashtag ;
                    rdfs:comment "Official hashtag of the electoral campaign." ;
                    rdfs:label "has official hashtag" .


###  http://w3id.org/speech#hasPoliticalCategory
:hasPoliticalCategory rdf:type owl:ObjectProperty ;
                      owl:inverseOf :politicalCategoryOf ;
                      rdfs:domain :PoliticalProposal ;
                      rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
                      rdfs:comment "The category to which a political proposal belongs." ;
                      rdfs:label "has political category" .


###  http://w3id.org/speech#ideologyOf
:ideologyOf rdf:type owl:ObjectProperty ;
            rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Speech
                                       :TwitterUser
                                       <http://xmlns.com/foaf/0.1/Agent>
                                     )
                       ] ;
            rdfs:comment "The elements that have this ideology. They can be both Agent-s and Speech-es." ;
            rdfs:label "ideology of" .


###  http://w3id.org/speech#officialhashtagOf
:officialhashtagOf rdf:type owl:ObjectProperty ;
                   rdfs:domain :Hashtag ;
                   rdfs:range :PoliticalPartyManifesto ;
                   rdfs:comment "The political manifesto that officially proposes the hashtag for the electoral campaign." ;
                   rdfs:label "official hashtag of" .


###  http://w3id.org/speech#politicalCategoryOf
:politicalCategoryOf rdf:type owl:ObjectProperty ;
                     rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> ;
                     rdfs:range :PoliticalProposal ;
                     rdfs:comment "Political proposals that address this issue." ;
                     rdfs:label "political category of" .


###  http://w3id.org/speech#proposedAsPoliticalCandidateBy
:proposedAsPoliticalCandidateBy rdf:type owl:ObjectProperty ;
                                owl:inverseOf :proposesPoliticalCandidate ;
                                rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                                rdfs:range :PoliticalPartyManifesto ;
                                rdfs:comment "The political manifesto that presents this agent as candidate to the elections." ;
                                rdfs:label "proposed as political candidate by" .


###  http://w3id.org/speech#proposesPoliticalCandidate
:proposesPoliticalCandidate rdf:type owl:ObjectProperty ;
                            rdfs:domain :PoliticalPartyManifesto ;
                            rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                            rdfs:comment "The political candidate presented by the political party in its manifesto for the electoral campaign." ;
                            rdfs:label "proposes political candidate" .


###  http://w3id.org/speech#retweetedBy
:retweetedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :retweets ;
             rdfs:domain :Tweet ;
             rdfs:range :TwitterUser ;
